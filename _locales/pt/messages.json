	},

	"textAboutOptions":{
		"message": "Este plugin envia o magnet link para download utilizando o Deluge-Web.<br>Deluge-Web é um recurso do próprio deluge, neste <a href=\"https://dev.deluge-torrent.org/wiki/UserGuide/ThinClient#WebUI\" target=\"_blank\">link</a> você encontrará mais informação sobre o deluge-web.",
		"description": "ID do contextMenu."
	},

	"aboutOptions":{
		"message": "Sobre:",
		"description": "ID do contextMenu."
	},

	"errorUrlOptions":{
		"message": "Não foi informado a url do deluge-web em options.",
		"description": "Erro caso não seja informado a url do deluge-web nas opções da extensão."
	},

	"errorQueryUrl": {
		"message": "Não foi possível encontrar uma aba para a url informada, $URL$ => $URLPATTERN$",
		"description": "Erro caso não encontre a aba do deluge-web",
		"placeholders": {
			"url" : {
		    	"content" : "$1",
		    	"example" : "http://delugeweb:port/"
		    },
		    "urlpattern":{
		    	"content" : "$2",
		      	"example" : "*://*.mozilla.org/*"
		    }
		}
	},

	"errorDelugeMethod":{
		"message": "Recebido o erro '$ERROR$' ao enviar o método '$METHOD$'",
		"description": "Erro exibido quando a resposta do deluge vier com erro informado.",
		"placeholders": {
			"error" : {
		    	"content" : "$1"
		    },
		    "method" : {
		    	"content" : "$2"
		    }
		}
	},

	"errorXmlHttpRequest":{
		"message": "Erro, status $STATUS$ ao tentar enviar o xmlHttpRequest $METHOD$",
		"description": "Erro exibido caso tenha algum problema ao enviar a request para o deluge.",
		"placeholders": {
			"status" : {
		    	"content" : "$1"
		    },
		    "method" : {
		    	"content" : "$2"
		    }
		}
	}

