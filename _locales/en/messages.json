			"error" : {
		    	"content" : "$1"
		    },
		    "method" : {
		    	"content" : "$2"
		    }
		}
	},

	"errorXmlHttpRequest":{
		"message": "Error, received status $STATUS$ when trying send xmlHttpRequest $METHOD$",
		"description": "Error when get some problem to send a request to deluge.",
		"placeholders": {
			"status" : {
		    	"content" : "$1"
		    },
		    "method" : {
		    	"content" : "$2"
		    }
